{
  "name": "@geckoai/ggen",
  "version": "1.0.13",
  "description": "This package is used to generate TS class files with the openAPI interface.",
  "main": "dist/cjs",
  "types": "dist/types",
  "type": "commonjs",
  "bin": {
    "ggen": "dist/cjs/bin/ggen.js"
  },
  "scripts": {
    "build:clean": "rimraf dist && rimraf docs",
    "build:cjs": "tsc --project tsconfig.prod.cjs.json",
    "build:types": "tsc --project tsconfig.prod.types.json",
    "build:doc": "typedoc --options ./typedoc.json",
    "release": "yarn build:clean &&  yarn build:cjs  && yarn build:types",
    "test": "mocha -w",
    "bin": "node ./dist/cjs/bin/ggen swagger -h",
    "bin:test": "ts-node ./src/bin/ggen swagger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geckoai/ggen.git"
  },
  "keywords": [
    "geckoai"
  ],
  "author": "YunlongRan<549510622@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geckoai/ggen/issues"
  },
  "homepage": "https://github.com/geckoai/ggen#readme",
  "dependencies": {
    "reflect-metadata": "^0.2.2",
    "@geckoai/class-mirror": "1.0.2",
    "@geckoai/class-transformer": "1.0.2",
    "@geckoai/gecko-core": "1.0.1-beta1",
    "@inquirer/prompts": "^7.5.3",
    "ansi-escapes": "^7.0.0",
    "axios": "^1.7.7",
    "ora": "^5.4.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "rxjs": "^7.8.1",
    "yaml": "^2.6.0",
    "commander": "13.1.0",
    "ts-morph": "~24.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@eslint/js": "^9.13.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@types/node": "16.18.97",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.4",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.11.0",
    "mocha": "^10.8.2",
    "moment": "^2.29.1",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.2",
    "typedoc": "^0.26.10",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.11.0"
  }
}
